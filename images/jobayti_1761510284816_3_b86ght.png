<?xml version="1.0" encoding="UTF-8"?>
<svg width="1200" height="630" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
  <defs>
    <!-- Gradients -->
    <linearGradient id="blueGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e3c72;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2a5298;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="yellowGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#f9d423;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff4e50;stop-opacity:1" />
    </linearGradient>
    
    <!-- Shadow filter -->
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="4"/>
      <feOffset dx="0" dy="4" result="offsetblur"/>
      <feFlood flood-color="#000000" flood-opacity="0.2"/>
      <feComposite in2="offsetblur" operator="in"/>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <filter id="buttonShadow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="3"/>
      <feOffset dx="0" dy="3" result="offsetblur"/>
      <feFlood flood-color="#000000" flood-opacity="0.3"/>
      <feComposite in2="offsetblur" operator="in"/>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#blueGradient)"/>
  
  <!-- Background pattern -->
  <circle cx="150" cy="150" r="100" fill="white" opacity="0.05"/>
  <circle cx="1050" cy="150" r="80" fill="white" opacity="0.05"/>
  <circle cx="200" cy="430" r="120" fill="white" opacity="0.03"/>
  <circle cx="1000" cy="480" r="90" fill="white" opacity="0.04"/>
  
  <!-- Title Section -->
  <text x="600" y="120" font-family="Arial, Tahoma, sans-serif" font-size="48" font-weight="bold" fill="white" text-anchor="middle">
    تطوير موقع لخدمة مدرس محادثة بالذكاء الا
  </text>
  <text x="600" y="170" font-family="Arial, Tahoma, sans-serif" font-size="42" font-weight="bold" fill="white" text-anchor="middle" opacity="0.95">
    صطناعي
  </text>
  
  <!-- Yellow underline -->
  <rect x="400" y="185" width="400" height="6" rx="3" fill="url(#yellowGradient)"/>
  
  <!-- Main content area -->
  <rect x="100" y="230" width="1000" height="280" rx="20" fill="white" opacity="0.95" filter="url(#shadow)"/>
  
  <!-- Platform info -->
  <g transform="translate(150, 270)">
    <rect x="0" y="0" width="200" height="60" rx="10" fill="#f0f4f8"/>
    <text x="100" y="25" font-family="Arial" font-size="16" fill="#718096" text-anchor="middle">المنصة</text>
    <text x="100" y="45" font-family="Arial" font-size="20" font-weight="bold" fill="#2d3748" text-anchor="middle">منصة مستقل</text>
  </g>
  
  <!-- Budget info -->
  <g transform="translate(380, 270)">
    <rect x="0" y="0" width="200" height="60" rx="10" fill="#f0f4f8"/>
    <text x="100" y="25" font-family="Arial" font-size="16" fill="#718096" text-anchor="middle">الميزانية</text>
    <text x="100" y="45" font-family="Arial" font-size="20" font-weight="bold" fill="#2d3748" text-anchor="middle">$250.00 - $500.00</text>
  </g>
  
  <!-- Bids info -->
  <g transform="translate(610, 270)">
    <rect x="0" y="0" width="200" height="60" rx="10" fill="#f0f4f8"/>
    <text x="100" y="25" font-family="Arial" font-size="16" fill="#718096" text-anchor="middle">عدد العروض</text>
    <text x="100" y="45" font-family="Arial" font-size="20" font-weight="bold" fill="#2d3748" text-anchor="middle">0 عرض</text>
  </g>
  
  <!-- Status info -->
  <g transform="translate(840, 270)">
    <rect x="0" y="0" width="200" height="60" rx="10" fill="#f0f4f8"/>
    <text x="100" y="25" font-family="Arial" font-size="16" fill="#718096" text-anchor="middle">الحالة</text>
    <text x="100" y="45" font-family="Arial" font-size="20" font-weight="bold" fill="#2d3748" text-anchor="middle">مفتوح</text>
  </g>
  
  <!-- Category info -->
  <g transform="translate(150, 350)">
    <rect x="0" y="0" width="300" height="60" rx="10" fill="#f0f4f8"/>
    <text x="150" y="25" font-family="Arial" font-size="16" fill="#718096" text-anchor="middle">التصنيف</text>
    <text x="150" y="45" font-family="Arial" font-size="20" font-weight="bold" fill="#2d3748" text-anchor="middle">برمجة، تطوير المواقع والتطبيقات</text>
  </g>
  
  <!-- Duration info -->
  <g transform="translate(480, 350)">
    <rect x="0" y="0" width="300" height="60" rx="10" fill="#f0f4f8"/>
    <text x="150" y="25" font-family="Arial" font-size="16" fill="#718096" text-anchor="middle">مدة التنفيذ</text>
    <text x="150" y="45" font-family="Arial" font-size="20" font-weight="bold" fill="#2d3748" text-anchor="middle">غير محدد</text>
  </g>
  
  <!-- Skills section -->
  
  <g transform="translate(150, 430)">
    <text x="0" y="0" font-family="Arial" font-size="16" fill="#718096">المهارات المطلوبة:</text>
    
      <g transform="translate(0, 10)">
        <rect x="0" y="0" width="160" height="35" rx="17" fill="#e6fffa" stroke="#38b2ac" stroke-width="1"/>
        <text x="80" y="23" font-family="Arial" font-size="14" font-weight="600" fill="#234e52" text-anchor="middle">
          برمجة مواقع
        </text>
      </g>
      <g transform="translate(180, 10)">
        <rect x="0" y="0" width="160" height="35" rx="17" fill="#e6fffa" stroke="#38b2ac" stroke-width="1"/>
        <text x="80" y="23" font-family="Arial" font-size="14" font-weight="600" fill="#234e52" text-anchor="middle">
          تطوير الويب
        </text>
      </g>
      <g transform="translate(360, 10)">
        <rect x="0" y="0" width="160" height="35" rx="17" fill="#e6fffa" stroke="#38b2ac" stroke-width="1"/>
        <text x="80" y="23" font-family="Arial" font-size="14" font-weight="600" fill="#234e52" text-anchor="middle">
          تصميم مواقع
        </text>
      </g>
      <g transform="translate(540, 10)">
        <rect x="0" y="0" width="160" height="35" rx="17" fill="#e6fffa" stroke="#38b2ac" stroke-width="1"/>
        <text x="80" y="23" font-family="Arial" font-size="14" font-weight="600" fill="#234e52" text-anchor="middle">
          خدمات الويب
        </text>
      </g>
  </g>
  
  
  <!-- Internet button -->
  <g transform="translate(500, 540)">
    <rect x="0" y="0" width="200" height="50" rx="25" fill="#4299e1" filter="url(#buttonShadow)" style="cursor: pointer;"/>
    <!-- Computer icon -->
    <g transform="translate(20, 12)">
      <rect x="0" y="0" width="24" height="18" rx="2" fill="white" stroke="none"/>
      <rect x="2" y="2" width="20" height="12" fill="#4299e1"/>
      <rect x="8" y="20" width="8" height="4" fill="white"/>
      <rect x="4" y="24" width="16" height="2" fill="white"/>
    </g>
    <text x="100" y="32" font-family="Arial" font-size="18" font-weight="bold" fill="white" text-anchor="middle">الانترنت</text>
  </g>
  
  <!-- Bottom left corner - jobayti.live -->
  <g transform="translate(100, 570)">
    <circle cx="10" cy="10" r="8" fill="#48bb78"/>
    <path d="M 6 10 L 10 14 L 14 6" stroke="white" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
    <text x="25" y="15" font-family="Arial" font-size="16" fill="white">jobayti.live</text>
  </g>
  
  <!-- Bottom right corner - Jobayti button -->
  <g transform="translate(1000, 570)">
    <rect x="0" y="0" width="100" height="40" rx="20" fill="#805ad5" filter="url(#buttonShadow)"/>
    <text x="50" y="26" font-family="Arial" font-size="16" font-weight="bold" fill="white" text-anchor="middle">Jobayti</text>
  </g>
</svg>